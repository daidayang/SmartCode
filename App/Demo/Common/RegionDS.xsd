<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="RegionDS" targetNamespace="http://tempuri.org/RegionDS.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/RegionDS.xsd" xmlns:mstns="http://tempuri.org/RegionDS.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop" >
	<xs:element name="RegionDS" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Region">
					<xs:complexType>
						<xs:sequence>
				<xs:element name="RegionID" type="xs:int"  minOccurs="0"/>
				<xs:element name="RegionDescription" type="xs:string"  minOccurs="0" msprop:nullValue="_null"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="RegionPK" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Region" />
			<xs:field xpath="mstns:RegionID" />
		</xs:unique>
	</xs:element>
</xs:schema>
